<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".TelaPrincipal" >

    <LinearLayout
        android:id="@+id/linearLayout1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical" >

        <FrameLayout
            android:id="@+id/my_frame"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:paddingBottom="@dimen/activity_vertical_margin"
            android:paddingLeft="@dimen/activity_horizontal_margin"
            android:paddingRight="@dimen/activity_horizontal_margin"
            android:paddingTop="@dimen/activity_vertical_margin"
            android:scaleType="fitCenter"
            tools:context=".MainActivity" >

            <ImageView
                android:id="@+id/image_areas"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:scaleType="fitCenter"
                android:src="@drawable/mascara_principal_frente3"
                android:visibility="invisible" />

            <ImageView
                android:id="@+id/image_areas_tras"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:scaleType="fitCenter"
                android:src="@drawable/mascara_principal_tras2"
                android:visibility="invisible" />

            <ImageView
                android:id="@+id/image"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:scaleType="fitCenter"
                android:src="@drawable/principal_frente" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/click"
                android:textSize="@dimen/tamanho20"
                android:layout_marginBottom="40dp"
                android:layout_marginRight="@dimen/tamanho20"
                android:id="@+id/textView"
                android:layout_gravity="right|bottom"
                android:textStyle="bold" />
        </FrameLayout>

        <HorizontalScrollView xmlns:android="http://schemas.android.com/apk/res/android"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:layout_weight="6">
            <!--android:scaleType="fitXY"-->
            <LinearLayout
                android:id="@+id/linearLayout2"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal" >

                <ImageButton
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:layout_marginLeft="5dp"
                    android:layout_gravity="center"
                    android:scaleType="centerCrop"
                    android:background="#FFFFFF"
                    android:id="@+id/imgIconeAbdomen"
                    android:src="@drawable/icone_abdomen"
                    />

                <ImageButton
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:layout_marginLeft="15dp"
                    android:layout_gravity="center"
                    android:scaleType="centerCrop"
                    android:background="#FFFFFF"
                    android:id="@+id/imgIconeBiceps"
                    android:src="@drawable/icone_biceps"/>

                <ImageButton
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:layout_marginLeft="15dp"
                    android:layout_gravity="center"
                    android:scaleType="centerCrop"
                    android:background="#FFFFFF"
                    android:id="@+id/imgIconeCostas"
                    android:src="@drawable/icone_costas"/>

                <ImageButton
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:layout_marginLeft="15dp"
                    android:layout_gravity="center"
                    android:scaleType="centerCrop"
                    android:background="#FFFFFF"
                    android:id="@+id/imgIconeCoxa"
                    android:src="@drawable/icone_coxa"/>

                <ImageButton
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:layout_marginLeft="15dp"
                    android:layout_gravity="center"
                    android:scaleType="centerCrop"
                    android:background="#FFFFFF"
                    android:id="@+id/imgIconeGluteo"
                    android:src="@drawable/icone_gluteo"/>

                <ImageButton
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:layout_marginLeft="15dp"
                    android:layout_gravity="center"
                    android:scaleType="centerCrop"
                    android:background="#FFFFFF"
                    android:id="@+id/imgIconeOmbro"
                    android:src="@drawable/icone_ombro"/>

                <ImageButton
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:layout_marginLeft="15dp"
                    android:layout_gravity="center"
                    android:scaleType="centerCrop"
                    android:background="#FFFFFF"
                    android:id="@+id/imgIconePanturrilha"
                    android:src="@drawable/icone_panturilha"/>

                <ImageButton
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:layout_marginLeft="15dp"
                    android:layout_gravity="center"
                    android:scaleType="centerCrop"
                    android:background="#FFFFFF"
                    android:id="@+id/imgIconePeito"
                    android:src="@drawable/icone_peito"/>

                <ImageButton
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:layout_marginLeft="15dp"
                    android:layout_gravity="center"
                    android:scaleType="centerCrop"
                    android:background="#FFFFFF"
                    android:id="@+id/imgIconeTriceps"
                    android:src="@drawable/icone_triceps"/>
        </LinearLayout>
    </HorizontalScrollView>
   <!-- <ListView
        android:id="@+id/list"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="center"
        android:layout_weight="6" >
    </ListView>-->
    </LinearLayout>

    <Button
        android:id="@+id/btnVirarFotoPrinc"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignParentRight="true"
        android:layout_centerInParent="true"
        android:background="@drawable/seta_direita2"
        android:text="@string/nada" />

    <Button
        android:id="@+id/btnVirarFotoPrinc2"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignParentLeft="true"
        android:layout_centerInParent="true"
        android:background="@drawable/seta_esquerda2"
        android:text="@string/nada" />

</RelativeLayout><!-- From: file:/C:/Users/Galdino/StudioProjects/academia_v10.1/app/src/main/res/layout/activity_tela_principal.xml -->